{
  "name": "node-assess-lowes",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "client": "cd vue-client && npm run dev",
    "server": "nodemon ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "node-fetch": "^2.1.2",
    "node-sass-middleware": "0.9.8",
    "nodemon": "1.11.0",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
